{
    "$schema": "http://json.schemastore.org/template",
    "author": "Mond Diolola",
    "classifications": ["webapi"],
    "name": "DotnetCore Api",
    "identity": "dotnetcore.api.template",
    "tags": {
        "language": "C#"
    },
    "sourceName": "dotnetcore.api.template",
    "preferNameDirectory": true,
    "symbols": {
      "localDockerImageName": {
        "type": "parameter",
        "isRequired": true,
        "datatype": "text",
        "replaces": "dotnetcoreapitemplate",
        "description": "provide your image name for locally running"
      }
    },
    "sources": [
      {
        "exclude": [
          "**/[Bb]in/**",
          "**/[Oo]bj/**",
          ".git/**",
          ".template.config/**/*",
          "**/*.filelist",
          "**/*.user",
          "**/*.lock.json",
          ".vs/**/*"
        ]
      }
    ]
  }